                     '+-----------------------------------------------------------------------------+
fc738     f107b408                   add     bufad,            #$08          ' offset to start sector LBA
fc73c     fb03a1da                   rdlong  vol_begin,        bufad         ' read
                     '+-----------------------------------------------------------------------------+

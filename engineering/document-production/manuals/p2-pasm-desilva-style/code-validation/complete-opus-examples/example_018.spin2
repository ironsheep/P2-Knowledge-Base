' Generated test program for example_018
        org     0
        
' Basic move
        mov     dest, source    ' dest = source
        mov     x, #42         ' x = 42 (immediate)
        mov     x, ##70000     ' x = 70000 (32-bit immediate)
' But wait, there's more!
        mvn     dest, source    ' dest = NOT source (inverted)
